 <input type=\"text\" :value=\"$store.state.user.username\" @input=\"setUsername($event.target.value)\">\n    </div>\n    <div class=\"input-row\">\n        <label for=\"email\">Email</label>\n        <input type=\"text\" :value=\"$store.state.user.email\" @input=\"setEmail($event.target.value)\">\n      </div>\n    <button @click=\"createUser()\">sign in</button>\n  </div>\n\n</template>\n\n<script>\nimport store from '../store/index'\n\nexport default {\n    store: store,\n\n    created(){\n        \n    },\n    methods: {\n        setUsername(username){\n            store.commit('setUsername', username)\n        },\n        setEmail(email){\n            store.commit('setEmail', email)\n        },\n        createUser(){\n            store.dispatch('createUser')\n        }\n    },\n    computed: {\n        store(){\n            return this.$store.state\n        }\n    }\n}\n</script>\n\n<style>\n.user {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 100%;\n    height: 100vh;\n}\n.input-row {\n    display: flex;\n    flex-direction: column;\n    height: 50px;\n    width: 200px;\n}\n\nbutton {\n    margin: 10px;\n}\nlabel {\n    margin: 5px\n}\n</style>"],"sourceRoot":"src/components"}]}